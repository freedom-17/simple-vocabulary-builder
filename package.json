{
  "name": "vocabulary_builder",
  "productName": "Simple-Vocabulary-Builder",
  "version": "1.0.0",
  "description": "Simple Vocabulary Builder",
  "main": "src/main.js",
  "scripts": {
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/demo.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . simple-vocabulary-builder --overwrite --platform=win32 --arch=x64 --icon=assets/demo.ico --prune=true --out=release-builds",
    "package-linux": "electron-packager . simple-vocabulary-builder --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/demo.png --prune=true --out=release-builds",
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "tg",
  "license": "ISC",
  "devDependencies": {
    "electron": "^4.2.5",
    "electron-installer-dmg": "^3.0.0",
    "electron-packager": "^13.1.1",
    "electron-winstaller": "^3.0.4",
    "rimraf": "^2.6.3"
  },
  "dependencies": {
    "electron-store": "^3.2.0",
    "handlebars": "^4.1.2",
    "jquery": "^3.4.0",
    "node-notifier": "^5.4.0"
  }
}
